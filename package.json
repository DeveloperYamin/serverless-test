{
    "name": "maple",
    "description": "The Data driven backoffice system for Shatez",
    "version": "1.0.0",
    "private": true,
    "author": "Shatez",
    "license": "MIT",
    "keywords": [
        "nextjs",
        "starter",
        "typescript"
    ],
    "scripts": {
        "dev": "next",
        "build": "next build",
        "start": "next start",
        "type-check": "tsc --noEmit",
        "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
        "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
        "postinstall": "husky install",
        "supabase:start": "pnpm supabase start",
        "supabase:status": "pnpm supabase status",
        "supabase:stop": "pnpm supabase stop",
        "supabase:types": "pnpm supabase gen types typescript --local --schema public > src/lib/db-generated.types.ts",
        "supabase:db": "pnpm supabase db diff",
        "test": "jest --watch",
        "test:ci": "jest --ci",
        "jest-coverage": "jest --coverage",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build",
        "reinstall-deps": "rm -rf node_modules && rm -rf pnpm-lock.yaml && pnpm install"
    },
    "lint-staged": {
        "./src/**/*.{ts,js,jsx,tsx}": [
            "eslint --ignore-path .gitignore --fix",
            "prettier --ignore-path .gitignore --write"
        ]
    },
    "dependencies": {
        "@auth/supabase-adapter": "0.1.13",
        "@hookform/resolvers": "3.3.2",
        "@radix-ui/react-accordion": "1.1.2",
        "@radix-ui/react-checkbox": "1.0.4",
        "@radix-ui/react-dialog": "1.0.5",
        "@radix-ui/react-dropdown-menu": "2.0.6",
        "@radix-ui/react-icons": "1.3.0",
        "@radix-ui/react-label": "2.0.2",
        "@radix-ui/react-progress": "1.0.3",
        "@radix-ui/react-scroll-area": "1.0.5",
        "@radix-ui/react-select": "2.0.0",
        "@radix-ui/react-slot": "1.0.2",
        "@radix-ui/react-toast": "1.1.5",
        "@radix-ui/themes": "2.0.1",
        "@supabase/supabase-js": "2.38.4",
        "@tanstack/react-table": "8.10.7",
        "class-variance-authority": "0.7.0",
        "clsx": "2.0.0",
        "jsonwebtoken": "9.0.2",
        "next": "14.0.2",
        "next-auth": "4.24.5",
        "next-themes": "0.2.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-hook-form": "7.48.2",
        "react-icons": "4.12.0",
        "tailwind-merge": "1.14.0",
        "tailwindcss-animate": "1.0.7",
        "uuid": "9.0.1",
        "zod": "3.22.4",
        "zustand": "4.4.7"
    },
    "devDependencies": {
        "@commitlint/cli": "18.0.0",
        "@commitlint/config-conventional": "18.0.0",
        "@faker-js/faker": "8.3.1",
        "@storybook/addon-essentials": "^7.5.3",
        "@storybook/addon-interactions": "^7.5.3",
        "@storybook/addon-links": "^7.5.3",
        "@storybook/addon-onboarding": "^1.0.8",
        "@storybook/addon-styling-webpack": "^0.0.5",
        "@storybook/blocks": "^7.5.3",
        "@storybook/nextjs": "^7.5.3",
        "@storybook/react": "^7.5.3",
        "@storybook/testing-library": "^0.2.2",
        "@tailwindcss/typography": "0.5.10",
        "@testing-library/jest-dom": "6.1.4",
        "@testing-library/react": "14.1.2",
        "@types/jest": "29.5.9",
        "@types/jsonwebtoken": "9.0.5",
        "@types/node": "20.8.8",
        "@types/react": "18.2.31",
        "@types/react-dom": "18.2.14",
        "@types/uuid": "9.0.7",
        "@typescript-eslint/eslint-plugin": "6.9.0",
        "@typescript-eslint/parser": "6.9.0",
        "autoprefixer": "10.4.16",
        "eslint": "8.52.0",
        "eslint-config-next": "14.0.2",
        "eslint-config-prettier": "9.0.0",
        "eslint-import-resolver-typescript": "3.6.1",
        "eslint-plugin-import": "2.29.0",
        "eslint-plugin-prettier": "5.0.1",
        "husky": "8.0.3",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "lint-staged": "15.0.2",
        "postcss": "8.4.31",
        "postcss-loader": "7.3.3",
        "prettier": "3.0.3",
        "reflect-metadata": "0.1.13",
        "storybook": "^7.5.3",
        "style-loader": "3.3.3",
        "supabase": "1.106.1",
        "tailwindcss": "3.3.5",
        "typescript": "5.2.2",
        "usehooks-ts": "2.9.1"
    },
    "eslintConfig": {
        "extends": [
            "plugin:storybook/recommended"
        ]
    }
}
